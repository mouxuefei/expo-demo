{
  "env": {
    "browser": true,
    "es2021": true,
    "react-native/react-native": true
  },
  "extends": [
    "plugin:react/recommended",
    "standard"
  ],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "plugins": [
    "react",
    "react-native"
  ],
  "rules": {
    "react-native/no-unused-styles": 2,
    "react-native/no-inline-styles": 1,
    "react-native/no-raw-text": 2,
    "react/react-in-jsx-scope": 0,
    "react/prop-types": 0,
    "no-tabs": 0,
    "no-mixed-spaces-and-tabs": 0
  }
}